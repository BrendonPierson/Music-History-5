
<ul id="sortable">
{{#each songs}}
<li class="ui-state-default" id="{{position}}">
  <div class="row song">
    <h3>{{name}}</h3>
    <div class="row">
      <p class="col-sm-4">Artist: {{artist}}</p>
      <p class="col-sm-4">Album: {{album}}</p>
      <div class="col-sm-4">
        <button  value="{{key}}" type="button" value="delete" class="btn btn-danger btn-sm">Delete Song</button>
      </div>
    </div>
  </div>
</li>
{{/each}}
</ul>